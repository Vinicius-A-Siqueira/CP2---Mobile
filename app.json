{
  "expo": {
    "name": "EcoConsciente",
    "slug": "EcoConsciente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8f5e9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecoconsciente.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e8f5e9"
      },
      "package": "com.ecoconsciente.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "App desenvolvido para o CP6 - Mobile | Sustentabilidade, dicas ecológicas e animações profissionais usando Expo + Moti.",
    "githubUrl": "https://github.com/Vinicius-A-Siqueira/CP2---Mobile"
  }
}

